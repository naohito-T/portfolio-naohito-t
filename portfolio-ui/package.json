{
  "name": "portfolio-web-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=\"local\" nuxt",
    "stg": "cross-env NODE_ENV=\"stg\" nuxt",
    "pro": "cross-env NODE_ENV=\"pro\" nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cross-env NODE_ENV=\"pro\" nuxt generate",
    "analyze": "cross-env NODE_ENV=\"dev\" nuxt build --analyze",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --fix --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "prettier": "prettier -w **/*.{scss,css}",
    "test": "jest",
    "cy:open": "cross-env NODE_PATH=src cypress open",
    "cy:run": "cross-env NODE_PATH=src cypress run --headless"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.24.6",
    "@nuxtjs/markdownit": "^2.0.0",
    "core-js": "^3.15.1",
    "cross-env": "^7.0.3",
    "firebase": "^8.1.2",
    "nuxt": "^2.15.7",
    "nuxt-user-agent": "^1.2.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@cypress/instrument-cra": "^1.4.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^27.0.1",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "cypress": "^8.3.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "jest": "^27.0.5",
    "node-sass": "^6.0.1",
    "nuxt-gsap-module": "^1.5.0",
    "nuxt-svg-loader": "^1.2.0",
    "prettier": "^2.3.2",
    "sass": "^1.36.0",
    "sass-loader": "10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.4.0",
    "vue-jest": "^3.0.4",
    "vuex-module-decorators": "^1.0.1"
